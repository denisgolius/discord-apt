{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":ignoreModulesAndTests",
    "replacements:all",
    "workarounds:all"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["*"],
      "groupName": "all dependencies",
      "groupSlug": "all",
      "separateMajorMinor": false,
      "automerge": true
    }
  ]
}
